# .env file for Docker

# Broker URL using the Docker service name 'rabbitmq'
BROKER_URL="amqp://guest:guest@rabbitmq:5672//"

# Celery Result Backend with the correct 'db+' prefix and password
RESULT_BACKEND="db+postgresql+psycopg://seo_audit_user:your_strong_password@postgres-db:5432/seo_audit_db"

# Main Database URL with the correct password
DATABASE_URL="postgresql+psycopg://seo_audit_user:your_strong_password@postgres-db:5432/seo_audit_db"